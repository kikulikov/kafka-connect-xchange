{
  "name": "xchange-connector",
  "config": {
    "connector.class": "io.connect.xchange.XChangeConnector",
    "kafka.topic": "market-data",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "xchange.poll.interval.ms": 20000,
    "xchange.market.known.name": "BINANCE",
    "xchange.market.class.name": "org.knowm.xchange.binance.BinanceExchange",
    "xchange.market.instruments": "ETH-BTC,BTC-DAI,BTC-TRY,SOL-BTC,LTC-BTC",
    "tasks.max": "2"
  }
}